public class Solution {
    public static void main(String[] args) throws IOException {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int arr[]=new int[101];
        for(int i=0;i<n;i++){
            int temp=sc.nextInt();
            arr[temp]++;
        }
        int total=0;
        for(int i=0;i<101;i++){
            total+=arr[i]/2;
        }
        System.out.println(total);
    }  
}
